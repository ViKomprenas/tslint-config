{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": {
            "options": [
                "single",
                "avoid-escape"
            ]
        },
        "max-classes-per-file": false,
        "member-ordering": false,
        "interface-name": false,
        "no-namespace": false,
        "array-type": [
            true,
            "array"
        ],
        "no-console": [true, "log"]
    },
    "rulesDirectory": []
}
